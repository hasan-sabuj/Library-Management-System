<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="584.0" prefWidth="824.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="libraryassistant.signup.ShowAllissuedBooksController">
   <children>
      <TableView fx:id="issuedBookstable" layoutX="147.0" layoutY="75.0" prefHeight="508.0" prefWidth="677.0">
        <columns>
          <TableColumn fx:id="titleColumn" prefWidth="221.0" text="Title" />
          <TableColumn fx:id="ISBNColumn" minWidth="0.0" prefWidth="145.0" text="ISBN" />
            <TableColumn fx:id="authorColumn" minWidth="0.0" prefWidth="162.0" text="Author" />
            <TableColumn fx:id="publisherColumn" minWidth="0.0" prefWidth="146.0" text="Publisher" />
        </columns>
      </TableView>
      <Label alignment="CENTER" layoutX="205.0" prefHeight="69.0" prefWidth="488.0" text="All issued books" textFill="#1d9729">
         <font>
            <Font name="System Bold" size="28.0" />
         </font>
      </Label>
      <Button layoutX="10.0" layoutY="196.0" mnemonicParsing="false" onMouseClicked="#backAction" prefHeight="45.0" prefWidth="131.0" text="Back" textFill="#1d8d30">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Button>
      <Button layoutX="9.0" layoutY="83.0" mnemonicParsing="false" onAction="#issueBookAction" prefHeight="45.0" prefWidth="131.0" text="Issue book" textFill="#22901d">
         <font>
            <Font name="System Bold" size="17.0" />
         </font>
      </Button>
      <Button layoutX="10.0" layoutY="139.0" mnemonicParsing="false" onMouseClicked="#returnbookAction" prefHeight="45.0" prefWidth="131.0" text="Return book" textFill="#23a130">
         <font>
            <Font name="System Bold" size="17.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
